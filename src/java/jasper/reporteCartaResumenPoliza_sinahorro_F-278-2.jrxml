	<queryString>
		<![CDATA[SELECT
	cer.numCertificado,
       	slc.apPaternoSolicitante, slc.apMaternoSolicitante, slc.nombre1Solicitante, slc.nombre2Solicitante, slc.fechaNacimientoSolicitante,
	pol.numPoliza, pol.numConsignatario, pol.sumaAseguradaIndividual,
	ts.nombreTipoSeguro,
        tar.importeTarifa, tar.importePrimaInd,
	prim.importePrimaColectiva, prim.sumaAseguradaColectiva,
        pla.cvePlaza, pla.domicilioPlaza, pla.lada, pla.telefono, pla.horarioAtencion,
        agt.cveAgente, agt.cveSupervisor, agt.cveGerente,
pol.sumaGastosFunerarios,pol.sumaSEVI, ts.nombreTipoSeguro, ret.cveDescuento
FROM
	CertificadoIndividual cer,
	Solicitante slc,
	PolizaIndividual pol,
	TipoSeguro ts,
     	TarifaAportMensual tar,
	PrimaMensualSeguroColectivo prim,
     	Solicitud sol, Plaza pla, Agente agt, Retenedor ret,
	Empresa  epr, GrupoAsegurado gpo
WHERE cer.numCertificado=$P{numCertificadoParam}
AND pol.numPoliza=$P{numPolizaParam}
AND cer.folioSolicitud = pol.folioSolicitud
AND cer.formatoSolicitud = pol.formatoSolicitud
AND sol.folioSolicitud=pol.folioSolicitud
AND sol.formatoSolicitud = pol.formatoSolicitud
AND slc.RFCsolicitante=sol.RFCsolicitante
AND tar.cveTarifa=sol.cveTarifa
AND agt.cveAgente=sol.cveAgente
AND pla.idPlaza=cer.idPlaza
AND prim.importePrimaColectiva=tar.importePrimaColectiva
AND ts.tipoSeguro=pol.tipoSeguro
AND ret.cveRetenedor=gpo.cveRetenedor
AND sol.cveEmpresa = epr.cveEmpresa
AND epr.cveGrupoAsegurado = gpo.cveGrupoAsegurado]]>
	</queryString>
